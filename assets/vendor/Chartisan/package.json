    "license": "MIT",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/node": "^12.12.21",
        "typedoc": "^0.16.5",
        "typescript": "^3.7.3",
        "ultrabundle": "^1.0.2"
    },
    "dependencies": {
        "deepmerge": "^4.2.2"
    }
}
